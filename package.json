{
  "name": "electron-hello-world",
  "version": "1.0.0",
  "description": "A basic Electron Hello World app",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run db:start && npm run build && electron .",
    "build": "tsc && webpack",
    "build:main": "tsc",
    "build:renderer": "webpack",
    "watch": "tsc -w",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down -v",
    "db:logs": "docker compose logs -f",
    "server:dev": "nodemon src/server/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/express": "^4.17.17",
    "@types/node": "^22.10.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.20",
    "css-loader": "^7.1.2",
    "electron": "^25.0.0",
    "electron-devtools-installer": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@apollo/client": "^3.12.4",
    "@types/pg": "^8.11.10",
    "@types/react-router-dom": "^5.3.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphiql": "^3.8.3",
    "pg": "^8.13.1",
    "postgraphile": "^4.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.1"
  }
}
